meta {
  name: Create scene
  type: http
  seq: 3
}

post {
  url: {{protocol}}://{{host}}/api/admin/scene
  body: json
  auth: none
}

body:json {
  {
    "name": "Testscreen",
    "triggerType": "time",
    "triggerData": "",
    "active": true
  }
}

vars:post-response {
  id: res.body.id
}

assert {
  res.body.name: eq Testscreen
}
